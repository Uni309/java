package FieldTest;

public class FieldExample {
	public static void main(String[] args) {
		FieldValue fiv = new FieldValue();
		
		System.out.println("byte 타입 필드선언:"+fiv.bytefield);
		System.out.println("char 타입 필드선언:"+fiv.charfield);
		System.out.println("short 타입 필드선언:"+fiv.shortfield);
		System.out.println("float 타입 필드선언:"+fiv.floatfield);
		System.out.println("boolean 타입 필드선언:"+fiv.booleanfield);
		System.out.println("배열 타입 필드선언:"+fiv.arrfield);
	}
}
